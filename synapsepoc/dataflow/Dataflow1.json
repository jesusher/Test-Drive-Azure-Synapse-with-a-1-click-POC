{
	"name": "Dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"linkedService": {
						"referenceName": "city_safety_seattle",
						"type": "LinkedServiceReference"
					},
					"name": "parquetsource"
				}
			],
			"sinks": [
				{
					"linkedService": {
						"referenceName": "adlsworspace",
						"type": "LinkedServiceReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     inferDriftedColumnTypes: true,",
				"     limit: 100,",
				"     ignoreNoFilesFound: true,",
				"     format: 'parquet',",
				"     container: 'citydatacontainer',",
				"     wildcardPaths:['*']) ~> parquetsource",
				"parquetsource sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'json',",
				"     fileSystem: 'jsonsource',",
				"     folderPath: '00',",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
			]
		}
	}
}